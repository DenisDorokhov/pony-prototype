<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="res" type="net.dorokhov.pony.web.client.Resources" />

	<g:FlowPanel styleName="{res.cssAlbumList.albumDisc}">
		<g:Label ui:field="captionLabel" styleName="{res.cssAlbumList.albumDiscHeader}" />
		<g:FlowPanel ui:field="songsPanel" styleName="{res.cssAlbumList.songList}" />
	</g:FlowPanel>

</ui:UiBinder>