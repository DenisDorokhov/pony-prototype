<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
			 xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:style src="ArtistsViewImpl.css" />

	<g:SplitLayoutPanel ui:field="rootContainer">
		<g:west size="200">
			<g:SimpleLayoutPanel ui:field="artistContainer">
				<c:CellList ui:field="artistList" styleName="{style.artistList}" />
			</g:SimpleLayoutPanel>
		</g:west>
		<g:center>
			<g:SimpleLayoutPanel ui:field="albumContainer">
				<g:Label>Albums</g:Label>
			</g:SimpleLayoutPanel>
		</g:center>
	</g:SplitLayoutPanel>

</ui:UiBinder>