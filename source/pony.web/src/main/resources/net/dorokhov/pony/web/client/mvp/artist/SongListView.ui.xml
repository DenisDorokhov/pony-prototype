<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="net.dorokhov.pony.web.client.Resources"></ui:with>

    <g:FlowPanel styleName="{res.style.albumDisc}">
        <g:Label ui:field="songListCaption" styleName="{res.style.albumDiscHeader}"></g:Label>
        <g:FlowPanel ui:field="songListPanel" styleName="{res.style.songList}"></g:FlowPanel>
    </g:FlowPanel>

</ui:UiBinder>