<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="net.dorokhov.pony.web.client.Resources"></ui:with>

    <g:FocusPanel ui:field="songItemPanel"
                  styleName="{res.style.songListItem}">
        <g:FlowPanel>
            <g:Label ui:field="trackNumberLabel" styleName="{res.style.trackNumber}"></g:Label>
            <g:Label ui:field="songNameLabel" styleName="{res.style.songName}"></g:Label>
            <g:Label ui:field="songDurationLabel" styleName="{res.style.songDuration}"></g:Label>
        </g:FlowPanel>
    </g:FocusPanel>

</ui:UiBinder>