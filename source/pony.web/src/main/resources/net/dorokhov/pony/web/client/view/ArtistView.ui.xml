<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="res" type="net.dorokhov.pony.web.client.Resources" />

	<g:FocusPanel ui:field="artistView" styleName="{res.cssArtistList.artistView}">
		<g:FlowPanel>
			<g:Image ui:field="artistImage" styleName="{res.cssArtistList.artistImage}" />
			<g:InlineLabel ui:field="artistName" styleName="{res.cssArtistList.artistName}" />
		</g:FlowPanel>
	</g:FocusPanel>

</ui:UiBinder>